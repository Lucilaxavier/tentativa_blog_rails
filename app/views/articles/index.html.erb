<h1>Listing articles</h1>

<table>
    <tr>
    <th>Title</th>
    <th>Text</th>
    <th></th>
    <th>

    <% @articles.each do |article| %>
        <tr>
        <td><%=article.Title %></td>
        <td><%= article.Text %></td>
        <td><%= link_to 'Show', article_path(article) %></td>
        </tr>
    <% end %>
</table>


<%= link_to 'new', new_article_path %>